{
  "name": "doessentials_app",
  "displayName": "doessentials_app",
  "expo": {
    "name": "Do Essentials",
    "slug": "doessentials-app",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doessentials.app"
    },
    "android": {
      "package": "com.doessentials.app",
      "permissions": ["com.android.vending.BILLING"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}